<p id="notice"><%= notice %></p>
<h1>Listing places</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Address</th>
      <th>Description</th>
      <th>Author</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @places.each do |place| %>
    <tr>
      <td><%= place.name %></td>
      <td><%= place.specification %></td>
      <td><%= place.address %></td>
      <td><%= place.description %></td>
      <td><%= place.user_id %></td>
      <td><%= link_to 'Show', admin_place_path(place) %></td>
      <td><%= link_to 'Edit', edit_admin_place_path(place) %></td>
      <td><%= link_to 'Destroy', admin_place_path(place), method: :delete, data: {confirm: 'Are you sure?'} %></td>
    </tr>
  <% end %>
  </tbody>
</table>


